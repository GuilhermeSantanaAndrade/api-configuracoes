{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "warn",
    "linebreak-style": "off",
    "space-before-function-paren": "off",
    "no-debugger": 0,
    "no-console": 0,
    "no-param-reassign": "off",
    "class-methods-use-this": "off",
    "func-names": [
      "warn",
      "never"
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "_"
      }
    ],
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}
